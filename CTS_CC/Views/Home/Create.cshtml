@model CTS_CC.Models.Member
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Create</h2>

@using (Html.BeginForm("CreateMember", "Home"))
{
    <div class="form-group">
        @Html.LabelFor(m => m._name)
        @Html.TextBoxFor(m => m._name, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m._address)
        @Html.TextBoxFor(m => m._address, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m._voterId)
        @Html.TextBoxFor(m => m._voterId, new { @class = "form-control" })
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
}
